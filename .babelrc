{
  "presets": [
    [
      "next/babel",
      {
        "preset-typescript": {
          "onlyRemoveTypeImports": true
        }
      }
    ]
  ],
  "plugins": [
    "babel-plugin-transform-typescript-metadata",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "babel-plugin-parameter-decorator"
  ]
}
